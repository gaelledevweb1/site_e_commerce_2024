{% extends 'base.html.twig' %}

{% block title %}Products
{% endblock %}

{% block content %}
	<h1>Products</h1>

	


	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ product.id |e  }}</td>
			</tr>
			<tr>
				<th>ArticleRef</th>
				<td>{{ product.articleRef |e  }}</td>
			</tr>
			<tr>
				<th>ArticleName</th>
				<td>{{ product.articleName |e }}</td>
			</tr>
			<tr>
				<th>ArticleImages</th>
				<td>{{ product.articleImages |e  }}</td>
			</tr>
			<tr>
				<th>ArticleThumbnails</th>
				<td>{{ product.articleThumbnails |e  }}</td>
			</tr>
			<tr>
				<th>ArticleStockQuantity</th>
				<td>{{ product.articleStockQuantity |e }}</td>
			</tr>
			<tr>
				<th>ArticleDescription</th>
				<td>{{ product.articleDescription |e  }}</td>
			</tr>
			<tr>
				<th>BoughtPrice</th>
				<td>{{ product.boughtPrice |e }}</td>
			</tr>
			<tr>
				<th>SellPriceHT</th>
				<td>{{ product.sellPriceHT |e  }}</td>
			</tr>
			<tr>
				<th>SellPriceTTC</th>
				<td>{{ product.sellPriceTTC  |e }}</td>
			</tr>
			<tr>
				<th>TVA</th>
				<td>{{ product.TVA  |e }}</td>
			</tr>
			<tr>
				<th>Details</th>
				<td>{{ product.details |e  }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_products_index') }}">back to list</a>

	
{% endblock %}
